<h1>Reviews#new</h1>
<%= simple_form_for @review, url: restaurant_reviews_path do |f| %>
  <div class="form-group">
    <%= f.text_field :content, class: 'form-control', placeholder: 'What you think about this restaurant' %>
  </div>

  <div class="form-group">
    <%= f.input :rating, collection: [1,2,3,4,5], class: 'form-control'%>
  </div>
  <%= f.submit class: 'btn btn-primary' %>
<% end %>
